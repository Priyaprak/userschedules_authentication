
<h1 class ="text-center mb-3">{{title}}</h1>

<h4 class ="mb-2">List of Schedules</h4>

<ul class="list-group">
    {{#if schedulesLength}}
        {{#each result}}
            <li class="list-group-item"><label> < {{this.username}}-{{this.daysoftheweek}} {{this.starttime}}-{{this.endtime}} > </label></li>
        {{/each}}
    {{else}}
        <p>No Schedules</p>
    {{/if}}
    
</ul>



